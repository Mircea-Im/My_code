# 최대 공약수 구하기
def max_gong(num1,num2):
    if num1 < num2 :
        for i in range(1,num1+1) :
            if num1%i == 0 and num2%i == 0 :
                gong = i
        return gong
    else:
        for j in range(1,num2+1) :
            if num1%i == 0 and num2%i == 0 :
                gong = i
        return gong

num1 = int(input('첫번째 수를 입력하세요 : '))
num2 = int(input('두번째 수를 입력하세요 : '))
print('{}과 {}의 최대 공약수는 {}입니다.'.format(num1,num2,max_gong(num1,num2)))
